pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building the application...'
            }
        }
        stage('test') {
            steps {
                echo 'testing the application...'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying the application...'
            }
        }
    }  // <- Closing brace for stages block
}      // <- Missing closing brace for the pipeline block

